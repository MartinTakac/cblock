BL_container_connector

BL_linear_transformation_connector

# weights saving
asomConsts/saveWeightsLead=asomConsts/qmlSaveWeights>asomConsts/saveWeightsPrev ? 1 : 0
asomConsts/saveWeightsPrev=asomConsts/qmlSaveWeights

asomConsts/loadWeightsLead=asomConsts/qmlLoadWeights>asomConsts/loadWeightsPrev ? 1 : 0
asomConsts/loadWeightsPrev=asomConsts/qmlLoadWeights

asom_save/time_index=asom_save/number_of_times-1
asom_save/retrieve=asomConsts/loadWeightsLead>0.?1.:0.

# xy som params
xy_input_som/learning_frequency_constants=xy_input_som/map_occupancy>0.95 ? 100*xy_som_consts/ones_layers : 1000*xy_som_consts/ones_layers
xy_input_som/compare_noise = xy_som_consts/qmlCompareNoise
xy_input_som/best_match_threshold = xy_som_consts/qmlBestMatchThreshold
xy_input_som/best_match_learning_multiplier = xy_som_consts/qmlBestMatchLfcMult
xy_input_som/activation_sensitivity = xy_som_consts/qmlSensitivity
xy_input_som/alphas = xy_som_consts/alphas
xy_input_som/sigma = xy_som_consts/qmlSigma
xy_input_som/training_record_decay = xy_som_consts/qmlTrainingRecordDecay

# bitmap som params
bitmap_input_som/learning_frequency_constants=bitmap_input_som/map_occupancy>0.95 ? 100*bitmap_som_consts/ones_layers : 1000*bitmap_som_consts/ones_layers
bitmap_input_som/compare_noise = bitmap_som_consts/qmlCompareNoise
bitmap_input_som/best_match_threshold = bitmap_som_consts/qmlBestMatchThreshold
bitmap_input_som/best_match_learning_multiplier = bitmap_som_consts/qmlBestMatchLfcMult
bitmap_input_som/activation_sensitivity = bitmap_som_consts/qmlSensitivity
bitmap_input_som/alphas = bitmap_som_consts/alphas
bitmap_input_som/sigma = bitmap_som_consts/qmlSigma
bitmap_input_som/training_record_decay = bitmap_som_consts/qmlTrainingRecordDecay

# in case of sparse input some values are overwritten 
BL_linear_transformation_connector
set_som_consts/perform_calculate=xy_som_consts_sparse/sparse_coded_input

random_generator/trigger=1
xy_input_som/bias_when_equal_activity=random_generator/values

xy_input_som/compare_noise = xy_som_consts_sparse/qmlCompareNoise
xy_input_som/best_match_threshold = xy_som_consts_sparse/qmlBestMatchThreshold
xy_input_som/best_match_learning_multiplier = xy_som_consts_sparse/qmlBestMatchLfcMult
xy_input_som/activation_sensitivity = xy_som_consts_sparse/qmlSensitivity
xy_input_som/sigma = xy_som_consts_sparse/qmlSigma
xy_input_som/training_record_decay = xy_som_consts_sparse/qmlTrainingRecordDecay

# in case of sparse input some values are overwritten 
BL_linear_transformation_connector
set_som_consts/perform_calculate=bitmap_som_consts_sparse/sparse_coded_input

random_generator/trigger=1
bitmap_input_som/bias_when_equal_activity=random_generator/values

bitmap_input_som/compare_noise = bitmap_som_consts_sparse/qmlCompareNoise
bitmap_input_som/best_match_threshold = bitmap_som_consts_sparse/qmlBestMatchThreshold
bitmap_input_som/best_match_learning_multiplier = bitmap_som_consts_sparse/qmlBestMatchLfcMult
bitmap_input_som/activation_sensitivity = bitmap_som_consts_sparse/qmlSensitivity
bitmap_input_som/sigma = bitmap_som_consts_sparse/qmlSigma
bitmap_input_som/training_record_decay = bitmap_som_consts_sparse/qmlTrainingRecordDecay

